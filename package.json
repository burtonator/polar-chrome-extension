{
    "name": "polar-chrome-extension",
    "version": "1.0.14",
    "description": "https://github.com/yeoman/generator-chrome-extension",
    "scripts": {
        "build": "npm run build:clean && webpack",
        "build:prod": "npm run build:clean && webpack --env.prod",
        "build:package": "npm run build:clean && webpack --env.prod --env.package",
        "build:clean": "rimraf ./extension",
        "watch": "npm run build:clean && webpack --watch",
        "watch:notif": "webpack --watch --env.notifs",
        "cache:clean": "rimraf ./node_modules/.cache",
        "publish-ext": "node publish",
        "test": "echo \"Error: no test specified\" && exit 1",
        "format": "prettier --config prettier.config.js --write **/*.{ts,js,tsx,jsx,css}"
    },
    "author": "",
    "license": "ISC",
    "dependencies": {
        "@fortawesome/fontawesome-free": "^5.5.0",
        "bootstrap": "^4.1.3",
        "react": "^16.7.0",
        "react-dom": "^16.7.0",
        "webextension-polyfill": "^0.3.0"
    },
    "devDependencies": {
        "@types/react": "^16.7.18",
        "babel-core": "^6.26.3",
        "babel-loader": "^7.1.4",
        "babel-preset-env": "^1.6.1",
        "babel-preset-react": "^6.24.1",
        "babel-preset-stage-1": "^6.24.1",
        "chrome-webstore-upload": "^0.2.2",
        "copy-webpack-plugin": "^4.5.1",
        "css-loader": "^0.28.11",
        "dotenv": "^6.0.0",
        "html-webpack-include-assets-plugin": "^1.0.4",
        "html-webpack-plugin": "^3.2.0",
        "mini-css-extract-plugin": "^0.4.0",
        "optimize-css-assets-webpack-plugin": "^5.0.0",
        "post-compile-webpack-plugin": "^0.1.2",
        "postcss-safe-parser": "^4.0.1",
        "prettier": "^1.13.7",
        "rimraf": "^2.6.2",
        "sign-addon": "^0.3.0",
        "style-loader": "^0.23.1",
        "svg-inline-loader": "^0.8.0",
        "thread-loader": "^1.1.5",
        "ts-loader": "^4.4.1",
        "tslint": "^5.11.0",
        "typescript": "^3.0.1",
        "uglifyjs-webpack-plugin": "^1.2.7",
        "url-loader": "^1.0.1",
        "webextension-polyfill-ts": "^0.8.8",
        "webpack": "^4.12.2",
        "webpack-build-notifier": "^0.1.25",
        "webpack-chrome-extension-reloader": "^0.8.3",
        "webpack-cli": "^3.1.0",
        "zip-webpack-plugin": "^3.0.0"
    }
}